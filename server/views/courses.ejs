    <br>
    <div class="p-4 p-md-5 mb-4 rounded text-body-emphasis bg-body-secondary info-hook">
      <div class="px-0">
        <h1 class="display-4 text">Demo Videos</h1>

        <div class="album py-5 bg-body-tertiary">
          <div class="container">
            <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
              <% for (let i=0; i < videos.length; i++) { %>
                <div class="col">
                  <div class="card shadow-sm videotab">
                    <iframe class="card-img-top" width="100%" height="225" src="<%=videos[i].url%>" title="demo video"
                      frameborder="0"
                      allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                      allowfullscreen>
                    </iframe>
                    <div class="card-body">
                      <h5 class="special-font-tab" color="var(--primary-color)">
                        <%= videos[i].title %>
                      </h5>
                      <p class="card-text special-font-tab" color="var(--primary-color)">
                        <%=videos[i].description%>
                      </p>
                      <div class="d-flex justify-content-between align-items-center">
                        <small class="text-body-secondary special-font-tab" color="var(--primary-color)">
                          <%=videos[i].duration%>
                        </small>
                      </div>
                    </div>
                  </div>
                </div>
              <% } %>
            </div>
          </div>
        </div>
      </div>

    </div>
    </div>
    <br>
    <div class="p-4 p-md-5 mb-4 rounded text-body-emphasis bg-body-secondary info-hook">
      <div class="px-0">
        <h1 class="display-4 text">Demo Notes</h1>
        <div class="table-responsive">
          <table class="table">
            <tr>
              <td>Sr.No</td>
              <td>Topic</td>
              <td>Course</td>
              <td>Download</td>
            </tr>
            <% for(let i=0;i<notes.length;i++) {%>
              <tr>
                <td><%=i+1%></td>
                <td><%=notes[i].topic%></td>
                <td><%=notes[i].course%></td>
                <td><a class="btn download-notes" href="<%=notes[i].url%>" style="color: var(--text-color);"
                  download><i class="fa-solid fa-file-pdf"></i>&nbsp;Download Notes</a></td>
              </tr>
            <%}%>
          </table>
        </div>
      </div>
